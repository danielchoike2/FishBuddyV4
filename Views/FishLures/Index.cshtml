@model IEnumerable<FishBuddy.Models.FishLure>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FishSpecies)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FishLureName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                    @Html.DisplayFor(modelItem => item.FishSpecies.FishCommonName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FishLureName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FishLureId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FishLureId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FishLureId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
